# Flutter App Environment Variables Template
# Copy this file to ../app/.env and update the values

# API Configuration
API_BASE_URL=http://localhost:3001/api/v1
PUBLIC_BASE_URL=http://localhost:3001/p

# Environment
ENVIRONMENT=development
DEBUG_MODE=true

# Feature Flags
ENABLE_EXTENSION_SUPPORT=true
ENABLE_IN_APP_BROWSER=false
ENABLE_SENTRY=false

# Sentry Configuration (optional)
SENTRY_DSN=your_flutter_sentry_dsn_here

# OAuth Configuration (when implemented)
GOOGLE_CLIENT_ID=your_google_client_id_here
APPLE_CLIENT_ID=your_apple_client_id_here

# App Configuration
APP_NAME=WishApp
APP_VERSION=0.1.0
MIN_SUPPORTED_VERSION=0.1.0
